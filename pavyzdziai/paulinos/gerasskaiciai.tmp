program Skaièiai;
   var   i,j,m,n,a:integer; f,fr:text;
{----------------------------------------------------------}
   procedure nulis(n,m:integer);
   begin
   for i:=1 to n do write(fr,'*');
   writeln(fr);
   for j:=1 to m-2 do begin
                      write(fr,'*');
                      for i:=1 to n-2 do write(fr,' ');
                      writeln(fr,'*')
                      end;
   for i:=1 to n do write(fr,'*');
   writeln(fr);
   end;
{----------------------------------------------------------}
   procedure vienas (n,m:integer);
   begin
   for i:=1 to m do begin
   for j:=1 to n-1 do
   write(fr,' ');
   writeln(fr,'*') ;
   end;
   end;
{----------------------------------------------------------}
  procedure du (n,m:integer);
  var p,l1,l2:integer;
  begin
  p:=m-3;
  l2:=p div 2;
  l1:=p-l2;
  for i:=1 to n do write(fr,'*');
  writeln(fr);
  for i:=1 to l2 do begin
                    for j:=1 to n-1 do write(fr,' ');
                    writeln(fr,'*')
                    end;
  for i:=1 to n do write(fr,'*');
  writeln(fr);
  for i:=1 to l1 do begin
                    write(fr,'*');
                    for j:=1 to n-1 do write(fr,' ');
                    writeln(fr)
                    end;
  for i:=1 to n do write(fr,'*');
  writeln(fr);
  end;
{----------------------------------------------------------}
  procedure trys (n,m:integer);
  var p,l1,l2:integer;
  begin
  p:=m-3;
  l2:=p div 2;
  l1:=p-l2;
  for i:=1 to n do write(fr,'*');
  writeln(fr);
  for i:=1 to l2 do begin
                    for j:=1 to n-1 do write(fr,' ');
                    writeln(fr,'*')
                    end;
  for i:=1 to n do write(fr,'*');
  writeln(fr);
  for i:=1 to l1 do begin
                    for j:=1 to n-1 do write(fr,' ');
                    writeln(fr,'*')
                    end;
  for i:=1 to n do write(fr,'*');
  writeln(fr);
  end;
{----------------------------------------------------------}
  procedure keturi(n,m:integer);
  var p,l1,l2:integer;
  begin
  p:=m-1;
  l2:=p div 2;
  l1:=p-l2;
  for j:=1 to l2 do begin
                    write(fr,'*');
                    for i:=1 to n-2 do write(fr,' ');
                    writeln(fr,'*')
                    end;
  for i:=1 to n do write(fr,'*');
  writeln(fr);
  for i:=1 to l1 do begin
                    for j:=1 to n-1 do write(fr,' ');
                    writeln(fr,'*')
                    end;
  end;
{----------------------------------------------------------}
  procedure penki(n,m:integer);
  var p,l1,l2:integer;
  begin
  p:=m-3;
  l2:=p div 2;
  l1:=p-l2;
  for i:=1 to n do write(fr,'*');
  writeln(fr);
  for j:=1 to l2 do begin
                    write(fr,'*');
                    for i:=1 to n-1 do write(fr,' ');
                    writeln(fr)
                    end;
  for i:=1 to n do write(fr,'*');
  writeln(fr);
  for i:=1 to l1 do begin
                         for j:=1 to n-1 do write(fr,' ');
                         writeln(fr,'*')
                         end;
  for i:=1 to n do write(fr,'*');
  writeln(fr);
  end;
{----------------------------------------------------------}
  procedure sesi(n,m:integer);
  var p,l1,l2:integer;
  begin
  p:=m-3;
  l2:=p div 2;
  l1:=p-l2;
  for i:=1 to n do write(fr,'*');
  writeln(fr);
  for i:=1 to l2 do begin
                    write(fr,'*');
                    for j:=1 to n-1 do write(fr,' ');
                    writeln(fr)
                    end;
  for i:=1 to n do write(fr,'*');
  writeln(fr);
  for j:=1 to l1 do begin
                    write(fr,'*');
                    for i:=1 to n-2 do write(fr,' ');
                    writeln(fr,'*')
                    end;
  for i:=1 to n do write(fr,'*');
  writeln(fr);
  end;
{----------------------------------------------------------}
  procedure septyni (n,m:integer);
  begin
  for i:=1 to n do write(fr,'*');
  writeln(fr);
  for i:=1 to m-1 do begin
                     for j:=1 to n-1 do write(fr,' ');
                     writeln(fr,'*')
                     end;
 end;
{----------------------------------------------------------}
 procedure astuoni(n,m:integer);
 var p,l1,l2:integer;
 begin
 p:=m-3;
 l2:=p div 2;
 l1:=p-l2;
 for i:=1 to n do write(fr,'*');
 writeln(fr);
 for j:=1 to l2 do begin
                   write(fr,'*');
                   for i:=1 to n-2 do write(fr,' ');
                   writeln(fr,'*');
                   end;
  for i:=1 to n do write(fr,'*');
  writeln(fr);
  for j:=1 to l1 do begin
                    write(fr,'*');
                    for i:=1 to n-2 do write(fr,' ');
                    writeln(fr,'*')
                    end;
  for i:=1 to n do write(fr,'*');
  writeln(fr);
  end;
 {----------------------------------------------------------}
  procedure devyni (n,m:integer);
  var p,l1,l2:integer;
  begin
  p:=m-3;
  l2:=p div 2;
  l1:=p-l2;
  for i:=1 to n do write(fr,'*');
  writeln(fr);
  for j:=1 to l2 do begin
                    write(fr,'*');
                    for i:=1 to n-2 do write(fr,' ');
                    writeln(fr,'*')
                    end;
  for i:=1 to n do write(fr,'*');
  writeln(fr);
  for i:=1 to l1 do begin
                    for j:=1 to n-1 do write(fr,' ');
                    writeln(fr,'*')
                    end;
  for i:=1 to n do write(fr,'*');
  writeln(fr);
  end;
 {----------------------------------------------------------}
begin
assign(f, 'duom.txt'); reset(f);
assign(fr, 'rez.txt'); rewrite(fr);
  while not eof(f) do
  begin
  readln(f,n);
  readln(f,m);
  while not eoln(f) do begin
                       readln(f,a);
                       if a=0 then nulis(n,m);
                       if a=1 then vienas(n,m);
                       if a=2 then du(n,m);
                       if a=3 then trys(n,m);
                       if a=4 then keturi(n,m);
                       if a=5 then penki(n,m);
                       if a=6 then sesi(n,m);
                       if a=7 then septyni(n,m);
                       if a=8 then astuoni(n,m);
                       if a=9 then devyni(n,m);
                       writeln(fr,'#######################');
                       end;
end;
  close(f);close(fr);
  Readln;Readln;
end.
